{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"news-market": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss",
					"skipTests": true
				},
				"@schematics/angular:class": {
					"skipTests": true
				},
				"@schematics/angular:directive": {
					"skipTests": true
				},
				"@schematics/angular:guard": {
					"skipTests": true
				},
				"@schematics/angular:interceptor": {
					"skipTests": true
				},
				"@schematics/angular:pipe": {
					"skipTests": true
				},
				"@schematics/angular:resolver": {
					"skipTests": true
				},
				"@schematics/angular:service": {
					"skipTests": true
				}
			},
			"root": "projects/news-market",
			"sourceRoot": "projects/news-market/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/news-market",
						"index": "projects/news-market/src/index.html",
						"main": "projects/news-market/src/main.ts",
						
						"tsConfig": "projects/news-market/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/news-market/src/favicon.ico",
							"projects/news-market/src/assets",
							{
								"glob": "**/*",
								"input": "./projects/core-package/src/lib/assets/icons",
								"output": "./assets/icons"
							}
						],
						"styles": [
							"node_modules/bootstrap-scss/bootstrap.scss",
							"projects/news-market/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/news-market/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/news-market/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "news-market:build:production",
							"extraWebpackConfig": "projects/news-market/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "news-market:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4200,
						"publicHost": "http://localhost:4200",
						"extraWebpackConfig": "projects/news-market/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "news-market:build",
						"extraWebpackConfig": "projects/news-market/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/news-market/tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/news-market/src/favicon.ico",
							"projects/news-market/src/assets"
						],
						"styles": [
							"projects/news-market/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		},
