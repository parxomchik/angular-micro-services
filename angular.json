    "core-package": {
      "projectType": "library",
      "root": "./projects/core-package",
      "sourceRoot": "./projects/core-package/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "./projects/core-package/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "./projects/core-package/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "./projects/core-package/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
